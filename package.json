{
  "name": "claudeflow",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:build:debug": "tauri build --debug",
    "tauri:build:all": "npm run tauri:build:windows && npm run tauri:build:mac && npm run tauri:build:linux",
    "tauri:build:windows": "tauri build --target x86_64-pc-windows-msvc",
    "tauri:build:mac": "tauri build --target universal-apple-darwin",
    "tauri:build:mac:intel": "tauri build --target x86_64-apple-darwin",
    "tauri:build:mac:arm": "tauri build --target aarch64-apple-darwin",
    "tauri:build:linux": "tauri build --target x86_64-unknown-linux-gnu",
    "tauri:build:linux:arm": "tauri build --target aarch64-unknown-linux-gnu",
    "clean": "rm -rf dist target src-tauri/target",
    "clean:all": "npm run clean && rm -rf node_modules",
    "lint": "tsc --noEmit",
    "lint:rust": "cd src-tauri && cargo clippy -- -D warnings",
    "format:rust": "cd src-tauri && cargo fmt",
    "test:rust": "cd src-tauri && cargo test",
    "check": "npm run type-check && npm run lint:rust",
    "postinstall": "node scripts/check-rust.js || true",
    "prebuild:check": "node scripts/check-deps.js",
    "build:local": "npm run prebuild:check && npm run tauri:build",
    "build:release": "npm run check && npm run tauri:build",
    "build:universal": "npm run tauri:build -- --target universal-apple-darwin",
    "dist:mac": "npm run tauri:build:mac && node scripts/notarize.js",
    "dist:win": "npm run tauri:build:windows",
    "dist:linux": "npm run tauri:build:linux",
    "type-check": "tsc --noEmit",
    "release": "npm run check && npm run tauri:build",
    "release:draft": "npm run release -- --config src-tauri/tauri.conf.json"
  },
  "dependencies": {
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.11",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-dialog": "^2.3.1",
    "@tauri-apps/plugin-fs": "^2.4.1",
    "@tauri-apps/plugin-opener": "^2",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.525.0",
    "postcss": "^8.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-resizable-panels": "^3.0.3",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "typescript": "~5.6.2",
    "vite": "^6.0.3"
  }
}
